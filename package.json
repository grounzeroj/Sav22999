{
  "name": "koa-cli-man",
  "version": "1.0.1",
  "description": "koa-cli-man,一款基于nodejs的新的web框架koa示例代码生成脚手架.",
  "type": "module",
  "main": "./bin/index.js",
  "bin": {
    "koa-cli-man": "bin/index.js",
    "kcman": "bin/index.js",
    "koa-cli": "bin/index.js"
  },
  "scripts": {
    "app": "node ./bin/index.js",
    "app-w": "nodemon bin/index.js",
    "app:test": "node tests/test.js",
    "lint": "eslint \"bin/**/*.js\" \"tests/**/*.js\"",
    "lint-fix": "eslint \"bin/**/*.js\" \"tests/**/*.js\" --fix",
    "test": "echo \\\"Error: no tests specified\\\" && exit 1"
  },
  "files": [
    "bin",
    "package.json",
    "README.md",
    ".gitignore"
  ],
  "engines": {
    "node": ">13.0.0"
  },
  "keywords": [
    "koa-cli",
    "koa",
    "cli",
    "nodejs",
    "node"
  ],
  "author": "LC ahlc@sina.cn",
  "homepage": "https://gitee.com/ahviplc/koa-cli",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.1",
    "ejs": "^3.1.6",
    "execa": "^5.1.1",
    "inquirer": "^8.1.1",
    "prettier": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^2.0.7",
    "standard": "^16.0.3"
  }
}
